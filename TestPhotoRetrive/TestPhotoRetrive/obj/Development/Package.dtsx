    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        int length = (int)Row.LargePhoto.Length;
        byte[] img = Row.LargePhoto.GetBlobData(0,length);
        MemoryStream ms = new MemoryStream(img);
        Image image = Image.FromStream(ms);
        Guid randomName = Guid.NewGuid();
        image.Save(@"C:/TestFolder/" + randomName +"."+ ImageFormat.Gif);
        Row.PhotoName = randomName.ToString()+ "."+ ImageFormat.Gif;
        return;


    }

